{
  "name": "Cyberpunked",
  "author": "Roman Bureacov",
  "editorScheme": "colors/Cyberpunk_Theme_Enhanced.xml",
  "dark": true,
  "colors": {
    "backgroundRedInactive": "#0f0e14",
    "backgroundRedPassive": "#39161c",
    "backgroundRedActive": "#7a2626",
    "foregroundRed": "#ff6259",
    "backgroundBluePassive": "#131220",
    "backgroundBlueActive": "#286871",
    "foregroundBluePrimary": "#64fdff",
    "foregroundBlueSecondary": "#3a7974",
    "colorDebug": "#ff00ff"
  },
  "ui": {
    "*": {
      "background": "backgroundRedPassive",
      "foreground": "foregroundRed"
    },

    "Banner": {

    },
    "Borders": {
      "color": "backgroundRedActive",
      "ContrastBorderColor": "colorDebug"
    },
    "Button": {
      "arc": 5,

      "default": {
        "startBackground": "backgroundRedActive",
        "endBackground": "backgroundRedPassive",
        "shadowColor": "backgroundRedActive",
        "foreground": "foregroundBluePrimary",
        "startBorderColor": "foregroundRed",
        "endBorderColor": "foregroundRed",
        "focusedBorderColor": "foregroundRed",
        "focusColor": "foregroundRed",
        "loadingForeground": "backgroundRedActive"
      },

      "Split": {
        "default.iconColor": "foregroundBluePrimary",
        "default.separatorColor": "forgroundBluePrimary"
      },

      "background": "backgroundBluePassive",
      "foreground": "backgroundBlueActive",
      "startBackground": "backgroundRedPassive",
      "endBackground": "backgroundRedPassive",
      "startBorderColor": "foregroundRed",
      "endBorderColor": "foregroundRed",
      "shadowColor": "backgroundRedActive",
      "disabledBorderColor": "backgroundRedActive",
      "disabledText": "backgroundRedActive",
      "shadowWidth": 2,
      "focusedBorderColor": "backgroundRedActive"

    },
    "ToolWindow": {
      "Button": {
        "hoverBackground": "backgroundBluePassive",
        "selectedForeground": "foregroundBluePrimary",
        "selectedBackground": "backgroundBlueActive"

